{
  "name": "create-ui-kit",
  "version": "0.1.0",
  "description": "Create framework-agnostic UI kits powered by js-template-engine",
  "main": "dist/index.js",
  "bin": {
    "create-ui-kit": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "ui-kit",
    "component-library",
    "react",
    "vue",
    "cli",
    "scaffolding"
  ],
  "dependencies": {
    "@js-template-engine/core": "workspace:*",
    "@js-template-engine/extension-react": "workspace:*",
    "@js-template-engine/extension-vue": "workspace:*",
    "@js-template-engine/extension-bem": "workspace:*",
    "@js-template-engine/types": "workspace:*",
    "commander": "^12.0.0",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/fs-extra": "^11.0.4",
    "@vitest/coverage-v8": "^1.3.1",
    "vitest": "^1.3.1"
  }
}
