{
  "name": "@js-template-engine/cli",
  "version": "1.0.1",
  "description": "CLI for JS Template Engine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "js-template-engine": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@js-template-engine/core": "workspace:*",
    "@js-template-engine/extension-bem": "workspace:*",
    "@js-template-engine/extension-tailwind": "workspace:*",
    "@js-template-engine/extension-react": "workspace:*",
    "@js-template-engine/extension-vue": "workspace:*",
    "@js-template-engine/extension-svelte": "workspace:*",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.3.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vitest": "^1.6.1"
  }
}